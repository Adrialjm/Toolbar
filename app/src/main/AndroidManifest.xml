<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.tooleverywhere">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/icononeko"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:roundIcon="@mipmap/icononeko_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:targetApi="31">
        <service android:enabled="true" android:name=".MusicService" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Usuario"
            android:exported="false"></activity>
        <activity
            android:name=".Busqueda"
            android:exported="false"></activity>
        <activity
            android:name=".Gato1"
            android:exported="false"></activity>
        <activity
            android:name=".Gato2"
            android:exported="false"></activity>
        <activity
            android:name=".Gato3"
            android:exported="false"></activity>
        <activity
            android:name=".Gato4"
            android:exported="false"></activity>
        <activity
            android:name=".Gato5"
            android:exported="false"></activity>
        <activity
            android:name=".CrearUsuario"
            android:exported="false"></activity>
        <activity
            android:name=".Rickroll"
            android:exported="false"></activity>
    </application>

</manifest>